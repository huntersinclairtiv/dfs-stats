    <script type="text/javascript">
    (function () {
        var app = angular.module("fantasydata")
            .directive('playerSearch', function ($timeout) {
                function link(scope, element, attrs) {
                    scope.PlayerSearchOptions = {
                        placeholder: "Search...",
                        dataTextField: "Name",
                        dataValueField: "PlayerID",
                        highlightFirst: false,
                        minLength: 3,
                        height: 300,
                        autoBind: false,
                        filter: "contains",
                        fixedGroupTemplate: "Last Played: #:data#",
                        template: kendo.template($("#playersearch_template").html()),
                        tagTemplate: kendo.template($("#playersearch_tag_template").html()),
                        select: function (e) {
                            if (!Array.isArray(scope.idProp))
                                scope.idProp = [];

                            var index = scope.idProp.indexOf(e.dataItem.PlayerID);
                            if (index < 0)
                                scope.idProp.push(e.dataItem.PlayerID);

                            scope.$apply();

                            if (scope.selectFn)
                                scope.selectFn(e);
                        },
                        deselect: function (e) {
                            var index = scope.idProp.indexOf(e.dataItem.PlayerID);
                            if (index > -1)
                                scope.idProp.splice(index, 1);

                            scope.$apply();

                            if (scope.selectFn)
                                scope.selectFn(e);
                        },
                        dataSource: {
                            serverFiltering: true,
                            group: { field: "Season", dir: "desc" },
                            sort: { field: "FantasyPoints", dir: "desc" },
                            requestStart: function (e) {
                                $(".player-search-combo .k-clear-value").removeClass("show-icon");
                                if (scope.FromFavorites) {
                                    e.preventDefault();
                                }
                            },
                            requestEnd: function (e) {
                                $(".player-search-combo .k-clear-value").addClass("show-icon");
                            },
                            transport: {
                                read: {
                                    type: "POST",
                                    dataType: "json",
                                    data: function () {
                                        return {
                                            searchtext: scope.player_search ? scope.player_search.input.val() : ''
                                        }
                                    },
                                    url: function () {
                                        if (typeof scope.searchUrl === "function")
                                            return scope.searchUrl();
                                        else return scope.searchUrl;
                                    }
                                }
                            }
                        },
                        value: scope.preset
                    }

                    scope.ClearPlayer = function () {
                        scope.idProp = [];
                        scope.txtProp = null;
                        scope.$apply();

                        var multiselect = $(player_search).data("kendoMultiSelect");
                        multiselect.value([]);

                        if (scope.clearFn)
                            scope.clearFn();
                    }

                    scope.AddFavorites = function (e) {
                        alert(scope.favProp);

                        
                    }

                    scope.resetFavorites = function () {
                        var leagueid = '';
                        if (leagueid != '') {
                            var leaguename = '';
                            if (confirm("Are you sure you want to reset your favorite " + leaguename + " players.")) {
                                $.post("/User/ResetFavorites", { leagueid }, function (response) {
                                    if (response) {
                                        toastr.options.positionClass = "toast-bottom-center";
                                        if (response) {
                                            $(".fav-player").find("i.fas").toggleClass('fas far');
                                            toastr.success("Your " + leaguename + " favorites have been reset");
                                        }
                                        else
                                            toastr.success("There was an error resetting your favorites...");
                                    }
                                });
                            }
                        }
                    }

                    $(document).ready(function () {
                        setTimeout(function () {
                            $(".player-search-combo .k-multiselect-wrap .k-input").attr("placeholder", "Search...");
                            $(".player-search-combo .k-clear-value").addClass("show-icon");

                            $(".player-search-combo .k-clear-value").unbind('click');
                            $(".player-search-combo .k-clear-value").click(function (e) {
                                scope.ClearPlayer();
                            });
                        }, 50, scope);
                    });

                }

                var template = $("#player-search").html();
                $("#player-search").remove();

 
                return {
                    restrict: 'E',
                    link: link,
                    template: template,
                    scope: {
                        idProp: '=',
                        favProp: '=',
                        preset: '=',
                        clearFn: '=',
                        selectFn: '=',
                        searchUrl: '='
                    }
                };
            });
    })();

  
</script>

