    <script id="sitewidesearch_template" type="text/x-kendo-template">
        <div class="player-container">
            #if((League == "NFL" && FantasyPosition == "DST") || League == "CFB" || League == "CBB"){#
            <div class="player-headshot-container">
                <img class="player-headshot" src="#: TeamLogoURL #" alt="#: Name #" />
            </div>
            <div class="player-info">
                <b>#: Name #</b><br />
                <span style="font-size: 12px;"> #: FantasyPosition # | #: Season #</span>
            </div>
            #} else {#
            <div class="player-headshot-container">
                <img class="player-headshot" src="https://s3-us-west-2.amazonaws.com/static.fantasydata.com/headshots/#: League.toLowerCase() #/low-res/#: HeadshotPlayerID #.png" alt="#: Name #" />
            </div>
            <div class="player-info">
                <b>#: Name #</b> <span style="font-size: 12px;">#: FantasyPosition #</span><br />
                <span style="font-size: 12px;">#: TeamName # | #: Season #</span>
            </div>
            #}#
            <div style="clear: both;"></div>
        </div>
    </script>

    <script>
        var app = angular.module("fantasydata");

        var MainLayoutController = function ($scope, $http, $timeout, screenSize) {
            var mainvm = this;

            mainvm.ShowSearch = false;
            mainvm.SetSearch = function (focus) {
                mainvm.ShowSearch = !mainvm.ShowSearch;
                if (focus)
                    $timeout(function () { $scope.player_search.element.focus() }, 50);
                else
                    $timeout(function () { $scope.player_search.element.blur() }, 50);
            }

            mainvm.HideSearch = function () {
                mainvm.ShowSearch = false;
                $scope.$apply();
            }

            $(document).on('click', function (e) {
                if ($(".site-search").find(e.target).length == 0) {
                    mainvm.HideSearch();
                }
            });

            mainvm.PlayerSearchOptions = {
                placeholder: "Search...",
                dataTextField: "Name",
                dataValueField: "PlayerID",
                highlightFirst: false,
                minLength: 3,
                height: 300,
                autoBind: false,
                filter: "contains",
                groupTemplate: "#:GetLeagueName(data)#",
                fixedGroupTemplate: "#:GetLeagueName(data)#",
                template: kendo.template($("#sitewidesearch_template").html()),
                select: function (e) {
                    window.location.href = e.dataItem.PlayerUrlString;
                },
                dataBound: function (e) {
                    var element = e.sender.element;
                    if ($(window).width() < 1199)
                        $(".k-animation-container").css({ left: 'auto', right: 0 - element.outerWidth() });
                },
                dataSource: {
                    serverFiltering: true,
                    group: { field: "LeagueSortOrder", dir: "asc" },
                    transport: {
                        read: {
                            type: "POST",
                            dataType: "json",
                            data: function () {
                                return {
                                    searchtext: $scope.player_search ? $scope.player_search.value() : ''
                                }
                            },
                            url: '/search'
                        }
                    }
                }
            }
        }

        function GetLeagueName(sort) {
            switch (sort) {
                case 100: return "NFL";
                case 200: return "MLB";
                case 300: return "NBA";
                case 400: return "NHL";
                default: return "Other";
            }
        }

        app.controller("MainLayoutController", MainLayoutController);
    </script>

